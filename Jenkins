pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/alihussain72/I191701_Task01.git'
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                sh 'npm test'
            }
        }
    }
}
